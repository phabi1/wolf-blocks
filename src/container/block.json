{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "wolf/container",
	"version": "0.1.0",
	"title": "Container",
	"category": "widgets",
	"icon": "smiley",
	"description": "Example block scaffolded with Create Block tool.",
	"example": {},
	"supports": {
		"html": false
	},
	"textdomain": "wolf",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js",
	"attributes": {
		"topStyle": {
			"type": "string",
			"default": "none"
		},
		"bottomStyle": {
			"type": "string",
			"default": "none"
		},
		"contentBgColor": {
			"type": "string",
			"default": "default"
		},
		"contentTextColor": {
			"type": "string",
			"default": "default"
		},
		"contentStyle": {
			"type": "string",
			"default": "none"
		}
	}
}
